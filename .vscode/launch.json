{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Development WEB",
			"request": "launch",
			"type": "dart",
			"program": "lib/main_development.dart",
			"args": [
				"--target",
				"lib/main_development.dart",
				"--web-port",
				"8080",
				"--web-browser-flag=--disable-web-security"
			]
		},
		{
			"name": "Production WEB",
			"request": "launch",
			"type": "dart",
			"program": "lib/main_production.dart",
			"args": [
				"--target",
				"lib/main_production.dart",
				"--web-port",
				"8080"
			]
		}
	],
	"compounds": []
}
